<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:FileDiff">

	<Viewbox x:Key="CompareIcon">
		<Canvas Width="100" Height="100">
			<Rectangle Canvas.Left="0" Canvas.Top="10" Width="40" Height="80">
				<Rectangle.Fill>
					<LinearGradientBrush EndPoint="1,1" StartPoint="0,0">
						<GradientStop Color="LightPink" Offset="0" />
						<GradientStop Color="#FFFF5252" Offset="1" />
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>
			<Rectangle Canvas.Left="50" Canvas.Top="0" Width="40" Height="80">
				<Rectangle.Fill>
					<LinearGradientBrush EndPoint="1,1" StartPoint="0,0">
						<GradientStop Color="LightGreen" Offset="0" />
						<GradientStop Color="#FF03C203" Offset="1" />
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>
			<Ellipse Stroke="#FF565656" StrokeThickness="12" Canvas.Left="15" Canvas.Top="15" Width="65" Height="65" />
			<Line Stroke="#FF565656" StrokeThickness="12" X1="70" Y1="70" X2="100" Y2="100" />
		</Canvas>
	</Viewbox>

	<Viewbox x:Key="PreviousDiffIcon">
		<Canvas Width="16" Height="15">
			<Path StrokeThickness="0">
				<Path.Fill>
					<LinearGradientBrush EndPoint="0.6,1" StartPoint="0.4,0">
						<GradientStop Color="#FF0AC30A" Offset="0" />
						<GradientStop Color="#FF004900" Offset="1" />
					</LinearGradientBrush>
				</Path.Fill>
				<Path.Data>
					<PathGeometry>
						<PathGeometry.Figures>
							<PathFigure StartPoint="8,4" IsClosed="True">
								<PathFigure.Segments>
									<LineSegment Point="15,12" />
									<LineSegment Point="1,12" />
								</PathFigure.Segments>
							</PathFigure>
						</PathGeometry.Figures>
					</PathGeometry>
				</Path.Data>
			</Path>
		</Canvas>
	</Viewbox>

	<Viewbox x:Key="NextDiffIcon">
		<Canvas Width="16" Height="15">
			<Path StrokeThickness="0">
				<Path.Fill>
					<LinearGradientBrush EndPoint="0.6,1" StartPoint="0.4,0">
						<GradientStop Color="#FF0AC30A" Offset="0" />
						<GradientStop Color="#FF004900" Offset="1" />
					</LinearGradientBrush>
				</Path.Fill>
				<Path.Data>
					<PathGeometry>
						<PathGeometry.Figures>
							<PathFigure StartPoint="1,3" IsClosed="True">
								<PathFigure.Segments>
									<LineSegment Point="15,3" />
									<LineSegment Point="8,11" />
								</PathFigure.Segments>
							</PathFigure>
						</PathGeometry.Figures>
					</PathGeometry>
				</Path.Data>
			</Path>
		</Canvas>
	</Viewbox>

	<Viewbox x:Key="FirstDiffIcon">
		<Canvas Width="16" Height="15">
			<Path StrokeThickness="0">
				<Path.Fill>
					<LinearGradientBrush EndPoint="0.6,1" StartPoint="0.4,0">
						<GradientStop Color="#FF0AC30A" Offset="0" />
						<GradientStop Color="#FF004900" Offset="1" />
					</LinearGradientBrush>
				</Path.Fill>
				<Path.Data>
					<PathGeometry>
						<PathGeometry.Figures>
							<PathFigure StartPoint="1,2" IsClosed="True">
								<PathFigure.Segments>
									<LineSegment Point="15,2" />
									<LineSegment Point="15,4" />
									<LineSegment Point="8,4" />
									<LineSegment Point="15,12" />
									<LineSegment Point="1,12" />
									<LineSegment Point="8,4" />
									<LineSegment Point="1,4" />
								</PathFigure.Segments>
							</PathFigure>
						</PathGeometry.Figures>
					</PathGeometry>
				</Path.Data>
			</Path>
		</Canvas>
	</Viewbox>

	<Viewbox x:Key="LastDiffIcon">
		<Canvas Width="16" Height="15">
			<Path StrokeThickness="0">
				<Path.Fill>
					<LinearGradientBrush EndPoint="0.6,1" StartPoint="0.4,0">
						<GradientStop Color="#FF0AC30A" Offset="0" />
						<GradientStop Color="#FF004900" Offset="1" />
					</LinearGradientBrush>
				</Path.Fill>
				<Path.Data>
					<PathGeometry>
						<PathGeometry.Figures>
							<PathFigure StartPoint="1,3" IsClosed="True">
								<PathFigure.Segments>
									<LineSegment Point="15,3" />
									<LineSegment Point="8,11" />
									<LineSegment Point="15,11" />
									<LineSegment Point="15,13" />
									<LineSegment Point="1,13" />
									<LineSegment Point="1,11" />
									<LineSegment Point="8,11" />
								</PathFigure.Segments>
							</PathFigure>
						</PathGeometry.Figures>
					</PathGeometry>
				</Path.Data>
			</Path>
		</Canvas>
	</Viewbox>

</ResourceDictionary>