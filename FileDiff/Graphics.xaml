<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:FileDiff">

	<DataTemplate x:Key="CompareIcon">
		<Viewbox>
			<Canvas Width="100" Height="100">
				<Rectangle Canvas.Left="0" Canvas.Top="10" Width="40" Height="80">
					<Rectangle.Fill>
						<LinearGradientBrush EndPoint="1,1" StartPoint="0,0">
							<GradientStop Color="LightPink" Offset="0" />
							<GradientStop Color="#FFFF5252" Offset="1" />
						</LinearGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
				<Rectangle Canvas.Left="50" Canvas.Top="0" Width="40" Height="80">
					<Rectangle.Fill>
						<LinearGradientBrush EndPoint="1,1" StartPoint="0,0">
							<GradientStop Color="LightGreen" Offset="0" />
							<GradientStop Color="#FF03C203" Offset="1" />
						</LinearGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
				<Ellipse Stroke="#FF565656" StrokeThickness="12" Canvas.Left="15" Canvas.Top="15" Width="65" Height="65" />
				<Line Stroke="#FF565656" StrokeThickness="12" X1="70" Y1="70" X2="100" Y2="100" />
			</Canvas>
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="SwapIcon">
		<Viewbox>
			<Canvas Width="16" Height="15">
				<Path StrokeThickness="0">
					<Path.Fill>
						<LinearGradientBrush EndPoint="0.6,1" StartPoint="0.4,0">
							<GradientStop Color="#FF7979FF" Offset="0" />
							<GradientStop Color="#FF0000B9" Offset="1" />
						</LinearGradientBrush>
					</Path.Fill>
					<Path.Data>
						<CombinedGeometry>
							<CombinedGeometry.Geometry1>
								<PathGeometry>
									<PathGeometry.Figures>
										<PathFigure StartPoint="3,0" IsClosed="True">
											<PathFigure.Segments>
												<LineSegment Point="10,4" />
												<LineSegment Point="3,8" />
											</PathFigure.Segments>
										</PathFigure>
									</PathGeometry.Figures>
								</PathGeometry>
							</CombinedGeometry.Geometry1>
							<CombinedGeometry.Geometry2>
								<PathGeometry>
									<PathGeometry.Figures>
										<PathFigure StartPoint="5,10" IsClosed="True">
											<PathFigure.Segments>
												<LineSegment Point="12,6" />
												<LineSegment Point="12,14" />
											</PathFigure.Segments>
										</PathFigure>
									</PathGeometry.Figures>
								</PathGeometry>
							</CombinedGeometry.Geometry2>
						</CombinedGeometry>
					</Path.Data>
				</Path>
			</Canvas>
		</Viewbox>
	</DataTemplate>

