<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:FileDiff">

	<Viewbox x:Key="CompareIcon">
		<Path Stretch="Uniform" Stroke="Black" Fill="Green" StrokeThickness="0">
			<Path.Data>
				<PathGeometry>
					<PathGeometry.Figures>
						<PathFigure StartPoint="0,0" IsClosed="True">
							<PathFigure.Segments>
								<LineSegment Point="20,20" />
								<LineSegment Point="0,40" />
							</PathFigure.Segments>
						</PathFigure>
					</PathGeometry.Figures>
				</PathGeometry>
			</Path.Data>
		</Path>
	</Viewbox>

	<Viewbox x:Key="PreviousDiffIcon">
		<Path Stretch="Uniform" Fill="Blue" StrokeThickness="0">
			<Path.Data>
				<PathGeometry>
					<PathGeometry.Figures>
						<PathFigure StartPoint="0,10" IsClosed="True">
							<PathFigure.Segments>
								<LineSegment Point="10,0" />
								<LineSegment Point="20,10" />
							</PathFigure.Segments>
						</PathFigure>
					</PathGeometry.Figures>
				</PathGeometry>
			</Path.Data>
		</Path>
	</Viewbox>

	<Viewbox x:Key="NextDiffIcon">
		<Path Stretch="Uniform" Fill="Blue" StrokeThickness="0">
			<Path.Data>
				<PathGeometry>
					<PathGeometry.Figures>
						<PathFigure StartPoint="0,10" IsClosed="True">
							<PathFigure.Segments>
								<LineSegment Point="10,20" />
								<LineSegment Point="20,10" />
							</PathFigure.Segments>
						</PathFigure>
					</PathGeometry.Figures>
				</PathGeometry>
			</Path.Data>
		</Path>
	</Viewbox>

</ResourceDictionary>